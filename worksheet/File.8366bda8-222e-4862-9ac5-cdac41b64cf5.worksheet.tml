      cardinality: MANY_TO_ONE
    - with: Site Type
      'on': '[VW_SITE_AS_IS::SITE_TYPE_CODE] = [VW_REF_SITE_TYPE_AS_IS::CODE]'
      type: INNER
      cardinality: MANY_TO_ONE
  - name: VW_NATIVE_VIDEO_INTERACTION
    alias: Native Video Interaction
    fqn: 77d88ced-b166-4eaa-9349-a761cc600964
  - name: VW_SITE_PARTICIPANTS_AS_IS
    alias: Membership Detail
    fqn: b2722dd8-098b-4234-8609-16d478a16996
    joins:
    - with: File Member Details
      'on': '[VW_SITE_PARTICIPANTS_AS_IS::USER_CODE] = [File Member Details::CODE]'
      type: INNER
      cardinality: MANY_TO_ONE
  - name: VW_REF_INTERACTION_TYPE_AS_IS
