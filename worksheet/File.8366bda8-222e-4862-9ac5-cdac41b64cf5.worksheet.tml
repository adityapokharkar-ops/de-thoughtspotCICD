guid: 8366bda8-222e-4862-9ac5-cdac41b64cf5
model:
  name: File
  description: File Analytics Datamodel
  model_tables:
  - name: VW_FILE_AS_IS
    alias: File
    fqn: 60e04cd9-0f68-4ab2-b85f-3f284aacb1bf
    joins:
    - with: File Creator Details
      'on': '[VW_FILE_AS_IS::FILE_CREATED_BY] = [File Creator Details::CODE]'
      type: INNER
      cardinality: MANY_TO_ONE
    - with: File Mapping
      'on': '[VW_FILE_AS_IS::CODE] = [File Mapping::FILE_CODE]'
      type: INNER
      cardinality: ONE_TO_MANY
    - with: Interaction
      'on': '[VW_FILE_AS_IS::CODE] = [VW_INTERACTION::FILE_CODE] AND [VW_FILE_AS_IS::TENANT_CODE] = [VW_INTERACTION::TENANT_CODE]'
      type: LEFT_OUTER
      cardinality: ONE_TO_MANY
    - with: Native Video Interaction
      'on': '[VW_FILE_AS_IS::CODE] = [VW_NATIVE_VIDEO_INTERACTION::FILE_CODE] AND [VW_FILE_AS_IS::TENANT_CODE] = [VW_NATIVE_VIDEO_INTERACTION::TENANT_CODE]'
      type: LEFT_OUTER
      cardinality: ONE_TO_MANY
    - with: Site
      'on': '[VW_FILE_AS_IS::SITE_CODE] = [VW_SITE_AS_IS::CODE]'
      type: INNER
      cardinality: MANY_TO_ONE
    - with: Tenant Details
      'on': '[VW_FILE_AS_IS::TENANT_CODE] = [VW_TENANT_DETAILS_AS_IS::TENANT_CODE]'
      type: INNER
      cardinality: MANY_TO_ONE
    - with: filter_options
      'on': '[VW_FILE_AS_IS::ACTIVE_FLAG] = [filter_options::TRUE]'
      type: INNER
      cardinality: MANY_TO_ONE
  - name: DATE_DIM
    alias: Interaction Date Dim
    fqn: 8c47a455-3610-4ea2-bf05-fdce683ef536
    joins:
    - with: Interaction
      'on': '[DATE_DIM::DATE] = [VW_INTERACTION::INTERACTION_DATETIME]'
      type: INNER
      cardinality: MANY_TO_ONE
  - name: file_type
    fqn: a8ea1227-072a-49b1-a487-e74fc04d17ac
  - name: VW_INTERACTION
    alias: Interaction
    fqn: 6f0f3599-22ca-4738-8bd2-038b0e3cfb7f
    joins:
    - with: Interaction Site
      'on': '[VW_INTERACTION::SITE_CODE] = [Interaction Site::CODE]'
      type: INNER
      cardinality: MANY_TO_ONE
    - with: Interaction User Details
      'on': '[VW_INTERACTION::INTERACTED_BY_USER_CODE] = [Interaction User Details::CODE]'
      type: INNER
      cardinality: MANY_TO_ONE
    - with: Site Participant Details
      'on': '[VW_INTERACTION::INTERACTED_BY_USER_CODE] = [VW_MEMBER_FOLLOWER_OTHER::USER_CODE]'
      type: INNER
      cardinality: MANY_TO_ONE
  - name: Author User Status
    alias: File Creator Details Status
    fqn: 3cfd1ca9-5308-4e42-b8ef-44384ef3e387
  - name: VW_MEMBER_FOLLOWER_OTHER
    alias: Site Participant Details
    fqn: de911c73-20ab-4b42-b8b0-e711127d1543
  - name: Interaction User Details
    fqn: 0b489297-d507-4734-979a-1839a6c85974
  - name: File Mapping
    fqn: 1ebd325f-9d3d-463d-b9de-69c389c26e76
  - name: VW_TENANT_DETAILS_AS_IS
    alias: Tenant Details
    fqn: 65fb81cb-a0c0-4a36-a972-9308693f7b1a
  - name: VW_SITE_AS_IS
    alias: Site
    fqn: 6d6e115b-0c82-4e62-9118-fc190203d1e9
    joins:
    - with: Membership Detail
      'on': '[VW_SITE_AS_IS::CODE] = [VW_SITE_PARTICIPANTS_AS_IS::SITE_CODE]'
