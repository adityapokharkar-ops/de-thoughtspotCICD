    fqn: 604b4ad8-275d-444e-b9b3-2a17577cdabd
    joins:
    - with: File
      'on': '[VW_REF_FILE_TYPE_AS_IS::CODE] = [VW_FILE_AS_IS::FILE_TYPE_CODE]'
      type: RIGHT_OUTER
      cardinality: ONE_TO_MANY
    - with: file_type
      'on': '[VW_REF_FILE_TYPE_AS_IS::PARENT_CATEGORY] = [file_type::FILE_TYPE]'
      type: LEFT_OUTER
      cardinality: MANY_TO_ONE
  - name: view_options
    fqn: 614c791a-fa45-4be5-b299-a617c69f82e8
    joins:
    - with: File
      'on': '[view_options::TRUE] = [VW_FILE_AS_IS::ACTIVE_FLAG]'
      type: INNER
      cardinality: ONE_TO_MANY
  - name: VW_REF_SITE_TYPE_AS_IS
    alias: Site Type
    fqn: e7fd46cf-6e44-4ca2-9ad9-4f6b314f6147
  - name: VW_SITE_CATEGORY_AS_IS
    alias: Site Category
    fqn: f9df5d04-482e-4ee6-b6f3-094a8f59667c
  - name: VW_REF_ENTITY_AS_IS
    alias: Entity Type
    fqn: afdaf7e1-6e8e-4d35-a01a-4a97633b406d
    joins:
    - with: Interaction
      'on': '[VW_REF_ENTITY_AS_IS::CODE] = [VW_INTERACTION::INTERACTION_ENTITY_CODE]'
      type: RIGHT_OUTER
      cardinality: ONE_TO_MANY
  - name: filter_options
    fqn: 15781957-4a71-42cc-afff-cd4c25bb0685
  - name: Interaction Site
    fqn: da3f355e-1112-48fc-a98f-344c549f669f
  - name: File Member Details
    fqn: 66228be6-e45d-4d4a-92eb-9d5d2a104f3c
  - name: File Creator Details
    fqn: b594a27f-b0b7-4024-b798-3e63140f1067
    joins:
    - with: File Creator Details Status
      'on': '[File Creator Details::STATUS_CODE] = [Author User Status::CODE]'
      type: INNER
      cardinality: MANY_TO_ONE
  formulas:
  - id: formula_File Downloads
    name: File Downloads
    expr: if ( [Interaction::FILE_CODE] != 'n/a' and [Interaction Type::DESCRIPTION] = 'download' ) then [Interaction::INTERACTED_BY_USER_CODE] else null
  - id: formula_File Hyperlink
    name: File Hyperlink
    expr: concat ( '<span style="color:gray;font-size:smaller;">' , [Ref File Type::PARENT_CATEGORY] , '</span><br>' , '{caption}' , [File::FILE_NAME] , '{/caption}' , [Tenant Details::DOMAIN_URL] , '/file_preview/' , [File::CODE] )
  - id: formula_File Modal Url
    name: File Modal Url
    expr: if ( [Tenant Details::DATA_SOURCE_CODE] = 'DS002' ) then ( if ( [Ref File Type::PARENT_CATEGORY] = 'Video' ) then concat ( '<span style="color:gray;font-size:smaller;">' , [Ref File Type::PARENT_CATEGORY] , '</span>' , '<br>' , concat ( "{caption}" , [File::FILE_NAME] , "{/caption}" , [Tenant Details::DOMAIN_URL] , "/file_preview/" , [File::CODE] , "?provider=native_video" ) ) else concat ( '<span style="color:gray;font-size:smaller;">' , [Ref File Type::PARENT_CATEGORY] , '</span><br>' , '{caption}' , [File::FILE_NAME] , '{/caption}' , [Tenant Details::DOMAIN_URL] , '/file_preview/' , [File::CODE] ) ) else ( if ( [Ref File Type::PARENT_CATEGORY] = 'Video' ) then concat ( '<span style="color:gray;font-size:smaller;">' , [Ref File Type::PARENT_CATEGORY] , '</span>' , '<br>' , concat ( "{caption}" , [File::FILE_NAME] , "{/caption}" , [Tenant Details::DOMAIN_URL] , "/apex/simpplr__app?u=/video/" , [File::PROVIDER_FILE_CODE] , "&fileId=" , [File::PROVIDER_FILE_CODE] , "&provider=native_video" ) ) else concat ( '<span style="color:gray;font-size:smaller;">' , [Ref File Type::PARENT_CATEGORY] , '</span><br>' , '{caption}' , [File::FILE_NAME] , '{/caption}' , [Tenant Details::DOMAIN_URL] , '/file_preview/' , [File::CODE] ) )
  - id: formula_File Name Category Text
    name: File Name Category Text
    expr: concat ( '<span style="color:gray;font-size:smaller;">' , [Ref File Type::PARENT_CATEGORY] , '</span>' , '<br>' , [File::FILE_NAME] , '</a>' )
  - id: formula_Interaction Count
    name: Interaction Count
    expr: count ( [Interaction::INTERACTED_BY_USER_CODE] )
  - id: formula_Interaction Datetime
    name: Interaction Datetime
    expr: if ( [Interaction::INTERACTION_DATETIME] = [Interaction::INTERACTION_DATETIME] ) then [Interaction::INTERACTION_DATETIME] else null
  - id: formula_Unique Views
    name: Unique Views
    expr: if ( [Interaction Type::DESCRIPTION] = 'view' and [Interaction::FILE_CODE] != 'n/a' ) then concat ( [Interaction::FILE_CODE] , [Interaction::INTERACTED_BY_USER_CODE] ) else null
  filters:
  - column:
    - Is File Directory?
    oper: in
    values:
    - 'false'
  - column:
    - File Storage Type Code
    oper: not in
    values:
    - fss005
  columns:
  - name: File Code
    description: This attribute represents the unique identifier of the file code.
    column_id: File::CODE
    properties:
      column_type: ATTRIBUTE
      custom_order:
      - k
      - h
  - name: Interaction Count
    description: This attribute represents the information of the Interaction Count.
    formula_id: formula_Interaction Count
    properties:
      column_type: MEASURE
      aggregation: SUM
      index_type: DONT_INDEX
  - name: File Name Category
    description: This attribute represents the information of the category name where the file was uploaded. For example, Screenshot123 and above it will display Image, meaning it is an image.
    formula_id: formula_File Name Category Text
    properties:
      column_type: ATTRIBUTE
      index_type: DONT_INDEX
  - name: Interaction Datetime
    description: This attribute represents the information of the datetime when the interaction was done.
    formula_id: formula_Interaction Datetime
    properties:
      column_type: ATTRIBUTE
      index_type: DONT_INDEX
  - name: File Url
    description: This attribute represents the information of the preview url of the file.  For example, when someone clicks on it , the link that opens up is of the file for the site or feed or to which the file was first uploaded.
    formula_id: formula_File Hyperlink
    properties:
      column_type: ATTRIBUTE
      index_type: DONT_INDEX
  - name: Unique Views
    description: This attribute represents the information used to count unique views on a file.
    formula_id: formula_Unique Views
    properties:
      column_type: ATTRIBUTE
      index_type: DONT_INDEX
  - name: File Downloads
