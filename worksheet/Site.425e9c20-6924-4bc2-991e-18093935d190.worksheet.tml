      cardinality: MANY_TO_ONE
    - with: Interaction User
      'on': '[VW_INTERACTION::INTERACTED_BY_USER_CODE] = [VW_USER_AS_IS::CODE] AND [VW_INTERACTION::TENANT_CODE] = [VW_USER_AS_IS::TENANT_CODE]'
      type: INNER
      cardinality: MANY_TO_ONE
    - with: Page Detail
      'on': '[VW_INTERACTION::PAGE_DETAIL_CODE] = [VW_REF_PAGE_DETAIL_AS_IS::CODE]'
      type: INNER
      cardinality: MANY_TO_ONE
