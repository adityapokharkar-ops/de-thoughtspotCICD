      'on': '[VW_INTERACTION::PAGE_GROUP_CODE] = [VW_REF_PAGE_GROUP_AS_IS::CODE]'
      type: INNER
      cardinality: MANY_TO_ONE
    - with: Receiver User Details
      'on': '[VW_INTERACTION::RECEIVER_USER_CODE] = [File Member Details::CODE]'
      type: INNER
      cardinality: MANY_TO_ONE
