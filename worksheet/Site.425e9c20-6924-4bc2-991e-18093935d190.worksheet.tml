      cardinality: MANY_TO_ONE
    - with: Site Participants User
      'on': '[VW_SITE_PARTICIPANTS_AS_IS::USER_CODE] = [user_view::CODE] AND [VW_SITE_PARTICIPANTS_AS_IS::TENANT_CODE] = [user_view::TENANT_CODE]'
