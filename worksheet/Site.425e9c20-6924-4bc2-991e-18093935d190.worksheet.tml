guid: 425e9c20-6924-4bc2-991e-18093935d190
model:
  name: Site
  model_tables:
  - name: VW_SITE_AS_IS
    alias: Site
    fqn: 6d6e115b-0c82-4e62-9118-fc190203d1e9
    joins:
    - with: Carousel Layout
      'on': '[VW_SITE_AS_IS::CAROUSEL_LAYOUT_TYPE_CODE] = [VW_REF_CAROUSEL_LAYOUT_TYPE_AS_IS::CODE]'
      type: INNER
      cardinality: MANY_TO_ONE
    - with: Daily Site Adoption
      'on': '[VW_SITE_AS_IS::TENANT_CODE] = [VW_DAILY_SITE_ADOPTION::TENANT_CODE] AND [VW_SITE_AS_IS::CODE] = [VW_DAILY_SITE_ADOPTION::SITE_CODE]'
      type: INNER
      cardinality: ONE_TO_MANY
    - with: File Storage Source
      'on': '[VW_SITE_AS_IS::FILE_STORAGE_TYPE_CODE] = [VW_REF_FILE_STORAGE_SOURCE_AS_IS::CODE]'
      type: INNER
      cardinality: MANY_TO_ONE
    - with: Interaction
      'on': '[VW_SITE_AS_IS::TENANT_CODE] = [VW_INTERACTION::TENANT_CODE] AND [VW_SITE_AS_IS::CODE] = [VW_INTERACTION::SITE_CODE]'
      type: LEFT_OUTER
      cardinality: ONE_TO_MANY
    - with: Landing Page Type
      'on': '[VW_SITE_AS_IS::LANDING_PAGE_TYPE_CODE] = [VW_REF_LANDING_PAGE_TYPE_AS_IS::CODE]'
      type: INNER
      cardinality: MANY_TO_ONE
    - with: Monthly Site Adoption
      'on': '[VW_SITE_AS_IS::TENANT_CODE] = [VW_MONTHLY_SITE_ADOPTION::TENANT_CODE] AND [VW_SITE_AS_IS::CODE] = [VW_MONTHLY_SITE_ADOPTION::SITE_CODE]'
      type: INNER
      cardinality: ONE_TO_MANY
    - with: Site Category
      'on': '[VW_SITE_AS_IS::TENANT_CODE] = [VW_SITE_CATEGORY_AS_IS::TENANT_CODE] AND [VW_SITE_AS_IS::SITE_CATEGORY_CODE] = [VW_SITE_CATEGORY_AS_IS::CODE]'
      type: INNER
      cardinality: MANY_TO_ONE
    - with: Site Participants
      'on': '[VW_SITE_AS_IS::CODE] = [VW_SITE_PARTICIPANTS_AS_IS::SITE_CODE] AND [VW_SITE_AS_IS::TENANT_CODE] = [VW_SITE_PARTICIPANTS_AS_IS::TENANT_CODE]'
      type: INNER
      cardinality: ONE_TO_MANY
    - with: Site Type
      'on': '[VW_SITE_AS_IS::SITE_TYPE_CODE] = [VW_REF_SITE_TYPE_AS_IS::CODE]'
      type: INNER
      cardinality: MANY_TO_ONE
    - with: Tenant Information
      'on': '[VW_SITE_AS_IS::TENANT_CODE] = [VW_TENANT_INFORMATION::CODE]'
      type: INNER
      cardinality: MANY_TO_ONE
