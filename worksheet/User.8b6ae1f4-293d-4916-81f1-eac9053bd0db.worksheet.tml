  formulas:
  - id: formula_Adoption Behaviour Preference
    name: Adoption Behaviour Preference
    expr: if ( [Daily Interaction Details::IS_CONTRIBUTOR] ) then 4 else if ( [Daily Interaction Details::IS_PARTICIPANT] ) then 3 else if ( [Daily Interaction Details::IS_OBSERVER] ) then 2 else if ( [Daily Interaction Details::HAS_LOGGED_IN] = false ) then 1 else 0
  - id: formula_Logged In User
    name: Logged In User
    expr: if ( [Daily Interaction Details::HAS_LOGGED_IN] ) then 1 else 0
  - id: formula_Monthly Snapshot Latest Month
    name: Monthly Snapshot Latest Month
    expr: if ( day ( today ( ) ) <= 3 and [Monthly Tenant Adoption Snapshot::REPORTING_MONTH] = add_months ( start_of_month ( today ( ) ) , - 2 ) ) then 'yes' else if ( day ( today ( ) ) > 3 and [Monthly Tenant Adoption Snapshot::REPORTING_MONTH] = add_months ( start_of_month ( today ( ) ) , - 1 ) ) then 'yes' else 'no'
  - id: formula_Parameter
    name: Parameter
    expr: if ( [User Parameter] = 'department' ) then [User::DEPARTMENT] else if ( [User Parameter] = 'location' ) then [User::LOCATION] else if ( [User Parameter] = 'segment' ) then [User::SEGMENT_NAME] else if ( [User Parameter] = 'user category' ) then [User::USER_CATEGORY_NAME] else [User::DEPARTMENT]
  - id: formula_Teams formula
    name: Teams formula
    expr: if ( [Teams parameter] = 'department' ) then [User::DEPARTMENT] else if ( [Teams parameter] = 'location' ) then [User::LOCATION] else if ( [Teams parameter] = 'people manager' ) then [User::FULL_NAME] else null
  - id: formula_Total Clickthrough
    name: Total Clickthrough
    expr: count ( [Search Result Click::SEARCH_TERM] )
  - id: formula_Total Clickthrough With Results
    name: Total Clickthrough With Results
    expr: count_if ( [Search::HAS_SEARCH_RESULT] = true , [Search Result Click::SEARCH_CODE] )
  - id: formula_Total Searches Term
    name: Total Searches Term
    expr: count ( [Search::SEARCH_TERM] )
  - id: formula_Total searches with no result
    name: Total searches with no result
    expr: count_if ( [Search::HAS_SEARCH_RESULT] = false , [Search Result Click::SEARCH_TERM] )
  - id: formula_Total searches with result
    name: Total searches with result
    expr: count_if ( [Search::HAS_SEARCH_RESULT] = true , [Search::SEARCH_TERM] )
  - id: formula_User Adoption Percentage
    name: User Adoption Percentage
    expr: unique_count_if ( [Daily Interaction Details::HAS_LOGGED_IN] , [Daily Interaction Details::USER_CODE] ) / group_unique_count ( [Daily Interaction Details::USER_CODE] , [User::TENANT_CODE] )
  columns:
  - name: About
    description: This attribute represents the userâ€™s bio or description text.
    column_id: User::ABOUT
    properties:
      column_type: ATTRIBUTE
      ai_context: User's biographical description or personal summary text, providing professional background and personal introduction for profile visitors.
  - name: Product
    column_id: Page Detail Reference::PRODUCT
    properties:
      column_type: ATTRIBUTE
