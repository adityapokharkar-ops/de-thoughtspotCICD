  formulas:
  - id: formula_Avg company values per recognition
    name: Avg company values per recognition
    expr: count ( [User Company Values::COMPANY_VALUE_CODE] ) / unique count ( [User Recognition::RECOGNITION_CODE] )
  - id: formula_Company Value
    name: Company Value
    expr: count ( [User Company Values::COMPANY_VALUE_CODE] )
  - id: formula_Is user allowance applicable
    name: Is user allowance applicable
    expr: 'if ( ( [User Recognition::IS_DELETED] and [User Rewards::IS_DELETED] ) or ( [User Rewards::IS_DELETED] = null and [User Recognition::IS_DELETED] ) ) then false else true '
  - id: formula_Offset End Date
    name: Offset End Date
    expr: add_days ( [Recognition end date] , - ( diff_days ( [Recognition end date] , [Recognition start date] ) + 1 ) )
  - id: formula_Offset Start Date
    name: Offset Start Date
    expr: add_days ( [Recognition start date] , - ( diff_days ( [Recognition end date] , [Recognition start date] ) + 1 ) )
  - id: formula_People - Recognition Given Total
    name: People - Recognition Given Total
    expr: unique_count_if ( [User Recognition::IS_DELETED] = false , [User Recognition::CODE] )
  - id: formula_People - Recognition Received Total
    name: People - Recognition Received Total
    expr: unique_count_if ( [User Recognition::IS_DELETED] = false , [User Recognition::CODE] )
  - id: formula_Recognition Given
    name: Recognition Given
    expr: unique_count_if ( [User Recognition::IS_DELETED] = false , [User Recognition::GIVER_USER_CODE] )
  - id: formula_Recognition Giver User Parameter
    name: Recognition Giver User Parameter
    expr: if ( [Giver User Parameter] = 'department' ) then [Giver User::DEPARTMENT] else if ( [Giver User Parameter] = 'location' ) then [Giver User::LOCATION]  else [Giver User::FULL_NAME]
  - id: formula_Recognition Received True Date
    name: Recognition Received True Date
    expr: date ( [User Recognition::RECEIVED_DATE] )
  - id: formula_Recognition Reciever User Parameter
    name: Recognition Reciever User Parameter
    expr: if ( [Reciever User Parameter] = 'department' ) then [Receiver User::DEPARTMENT] else if ( [Reciever User Parameter] = 'location' ) then [Receiver User::LOCATION] else [Receiver User::FULL_NAME]
  - id: formula_Users who gave recognition - numerator
    name: Users who gave recognition - numerator
    expr: unique_count_if ( [Giver User Status::DESCRIPTION] = 'active' and [User Recognition::IS_DELETED] = false , [User Recognition::GIVER_USER_CODE] )
  - id: formula_Users who got recognized - numerator
    name: Users who got recognized - numerator
    expr: unique_count_if ( [Receiver User Status::DESCRIPTION] = 'active' and [User Recognition::IS_DELETED] = false , [User Recognition::RECEIVER_USER_CODE] )
