    - with: Award Status
      'on': '[VW_AWARD_INSTANCE_AS_IS::STATUS_CODE] = [VW_REF_AWARD_STATUS_AS_IS::CODE]'
      type: INNER
      cardinality: MANY_TO_ONE
    - with: Award Type
      'on': '[VW_AWARD_INSTANCE_AS_IS::AWARD_TYPE_CODE] = [VW_REF_AWARD_TYPE_AS_IS::CODE]'
      type: INNER
      cardinality: MANY_TO_ONE
