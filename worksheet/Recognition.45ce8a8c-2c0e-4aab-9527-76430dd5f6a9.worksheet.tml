model:
  name: Recognition
  model_tables:
  - name: VW_USER_AWARDS_AS_IS
    alias: User Recognition
    fqn: 8243cc66-befd-4dca-a78e-2c447d09394f
    joins:
    - with: Receiver User
      'on': '[VW_USER_AWARDS_AS_IS::TENANT_CODE] = [VW_USER_AS_IS::TENANT_CODE] AND [VW_USER_AWARDS_AS_IS::RECEIVER_USER_CODE] = [VW_USER_AS_IS::CODE]'
      type: INNER
      cardinality: MANY_TO_ONE
    - with: Recognition Details
      'on': '[VW_USER_AWARDS_AS_IS::TENANT_CODE] = [VW_AWARD_INSTANCE_AS_IS::TENANT_CODE] AND [VW_USER_AWARDS_AS_IS::AWARD_CODE] = [VW_AWARD_INSTANCE_AS_IS::AWARD_CODE]'
      type: INNER
      cardinality: MANY_TO_ONE
    - with: Tenant Information
      'on': '[VW_USER_AWARDS_AS_IS::TENANT_CODE] = [VW_TENANT_INFORMATION::CODE]'
      type: INNER
      cardinality: MANY_TO_ONE
    - with: User Company Values
      'on': '[VW_USER_AWARDS_AS_IS::CODE] = [VW_USER_COMPANY_VALUES::USER_AWARD_CODE] AND [VW_USER_AWARDS_AS_IS::TENANT_CODE] = [VW_USER_COMPANY_VALUES::TENANT_CODE]'
      type: LEFT_OUTER
      cardinality: ONE_TO_MANY
    - with: User Rewards
      'on': '[VW_USER_AWARDS_AS_IS::CODE] = [VW_USER_REWARDS_AS_IS::CODE] AND [VW_USER_AWARDS_AS_IS::TENANT_CODE] = [VW_USER_REWARDS_AS_IS::TENANT_CODE]'
      type: INNER
      cardinality: ONE_TO_ONE
