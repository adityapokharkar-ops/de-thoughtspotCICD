    - with: Reciever manager
      'on': '[VW_USER_AS_IS::TENANT_CODE] = [Reciever manager::TENANT_CODE] AND [VW_USER_AS_IS::CODE] = [Reciever manager::MANAGER_CODE]'
      type: INNER
