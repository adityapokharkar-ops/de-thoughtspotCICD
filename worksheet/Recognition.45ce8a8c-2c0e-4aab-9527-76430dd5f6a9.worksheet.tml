      type: INNER
      cardinality: MANY_TO_ONE
  - name: VW_USER_AS_IS
    alias: Giver User
    fqn: 38306036-94a8-4c22-a7f8-64d6c854732e
    joins:
    - with: Giver User Status
      'on': '[VW_USER_AS_IS::STATUS_CODE] = [VW_REF_USER_STATUS_AS_IS::CODE]'
      type: LEFT_OUTER
      cardinality: MANY_TO_ONE
    - with: Giver manager
      'on': '[VW_USER_AS_IS::TENANT_CODE] = [Giver manager::TENANT_CODE] AND [VW_USER_AS_IS::CODE] = [Giver manager::MANAGER_CODE]'
      type: LEFT_OUTER
      cardinality: MANY_TO_ONE
    - with: User Recognition
      'on': '[VW_USER_AS_IS::TENANT_CODE] = [VW_USER_AWARDS_AS_IS::TENANT_CODE] AND [VW_USER_AS_IS::CODE] = [VW_USER_AWARDS_AS_IS::GIVER_USER_CODE]'
      type: LEFT_OUTER
